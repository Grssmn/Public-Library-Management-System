<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="319.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bookmanagementproject.DeleteUser">
   <children>
      <Label layoutX="25.0" layoutY="22.0" prefHeight="28.0" prefWidth="269.0" text="Are you sure to delete the user?">
         <font>
            <Font name="Cochin" size="20.0" />
         </font>
      </Label>
      <HBox layoutX="49.0" layoutY="75.0" prefHeight="45.0" prefWidth="222.0" spacing="20.0">
         <children>
            <Button fx:id="delete" mnemonicParsing="false" onAction="#delete" prefHeight="44.0" prefWidth="104.0" styleClass="login-save" stylesheets="@loginDesign.css" text="Delete" textFill="WHITE">
               <font>
                  <Font name="Cochin" size="24.0" />
               </font>
            </Button>
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel" prefHeight="44.0" prefWidth="105.0" styleClass="login-cancel" stylesheets="@loginDesign.css" text="Cancel" textFill="WHITE">
               <font>
                  <Font name="Cochin" size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
